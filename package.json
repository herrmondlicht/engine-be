{
  "name": "engine-be",
  "version": "1.0.0",
  "description": "Backend for Engine project",
  "main": "index.js",
  "scripts": {
    "start-dev": "nodemon --exec babel-node src/index.js",
    "start": "node build/index.js",
    "build": "npm run migration && rm -rf ./build && ./node_modules/.bin/babel src -d build",
    "test": "jest --watchAll",
    "set-db": "bash ./database_structure/docker-creation.sh",
    "reset-db": "docker rm -f engine-mysql && bash ./database_structure/docker-creation.sh",
    "seed-db": "npm run migration && knex seed:run --specific=index.js",
    "migration": "npx knex migrate:latest"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/herrmondlicht/engine-be.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/herrmondlicht/engine-be/issues"
  },
  "homepage": "https://github.com/herrmondlicht/engine-be#readme",
  "dependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/runtime": "^7.10.3",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.11",
    "mysql": "^2.18.1",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "babel-jest": "^26.6.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "faker": "github:marak/Faker.js",
    "jest": "^26.6.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  }
}
