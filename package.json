{
  "name": "engine-be",
  "version": "1.0.0",
  "description": "Backend for Engine project",
  "engines": {
    "node": "14.x"
  },
  "main": "index.js",
  "scripts": {
    "start-dev": "nodemon --exec babel-node src/index.js",
    "start": "node build/index.js",
    "build": "npm run migration && rm -rf ./build && ./node_modules/.bin/babel src -d build && cp -r src/template build/template && cp -r src/public build/public",
    "test": "jest --watchAll",
    "set-db": "bash ./database_structure/docker-creation.sh",
    "reset-db": "docker rm -f engine-mysql && bash ./database_structure/docker-creation.sh",
    "seed-db": "npm run migration && knex seed:run --specific=index.js",
    "migration": "npx knex migrate:latest"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/herrmondlicht/engine-be.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/herrmondlicht/engine-be/issues"
  },
  "homepage": "https://github.com/herrmondlicht/engine-be#readme",
  "dependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@types/jest": "^27.4.1",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.1",
    "core-js": "^3.20.2",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "ejs": "^3.1.7",
    "express": "^4.17.2",
    "express-jwt": "^6.1.0",
    "html-pdf-node": "^1.0.8",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.15",
    "mysql": "^2.18.1",
    "regenerator-runtime": "^0.13.9",
    "sinon": "^13.0.1"
  },
  "devDependencies": {
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.25.4",
    "faker": "5.5.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1"
  }
}
