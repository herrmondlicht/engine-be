{
  "name": "engine-be",
  "version": "1.0.0",
  "description": "Backend for Engine project",
  "main": "index.js",
  "scripts": {
    "start-dev": "nodemon --exec babel-node src/index.js",
    "start": "node build/index.js",
    "build": "rm -rf ./build && babel src -d build",
    "test": "jest --watchAll",
    "set-db": "bash ./database_structure/docker-creation.sh",
    "reset-db": "docker rm -f engine-mysql && bash ./database_structure/docker-creation.sh",
    "db-seed": "knex seed:run --specific=index.js"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/herrmondlicht/engine-be.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/herrmondlicht/engine-be/issues"
  },
  "homepage": "https://github.com/herrmondlicht/engine-be#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.14",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "faker": "github:marak/Faker.js",
    "jest": "^24.9.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5"
  }
}
